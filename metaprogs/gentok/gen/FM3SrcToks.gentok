   Long_Xor            "Xor" . 
   Long_Not            "Not" . 
   Long_Shift          "Shift" . 
   Long_LeftShift      "LeftShift" . 
   Long_RightShift     "RightShift" . 
   Long_Rotate         "Rotate" . 
   Long_LeftRotate     "LeftRotate" . 
   Long_RightRotate    "RightRotate" . 
   Long_Extract        "Extract" . 
   Long_Insert         "Insert" . 

   TokMaxPredef        "<TokMaxPredef>" .
   
(* End of FM3SrcToks.gentok. *) 
